{
  "version": 2,
  "builds": [
    {
      "src": "src/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "src/serverless.js" },
    { "source": "/api-docs", "destination": "src/serverless.js" },
    { "source": "/(.*)", "destination": "src/serverless.js" }
  ]
}